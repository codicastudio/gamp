{
    "name": "codicastudio/gamp",
    "description": "A random Codica Studio package.",

    "homepage": "https://github.com/codicastudio/gamp",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael J Brown",
            "email": "mbrown@codicastudio.com",
            "homepage": "https://codicastudio.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "illuminate/support": "^8.0",
        "theiconic/php-ga-measurement-protocol": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Irazasyed\\LaravelGAMP\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Irazasyed\\LaravelGAMP\\LaravelGAMPServiceProvider"
            ],
            "aliases": {
                "GAMP": "Irazasyed\\LaravelGAMP\\Facades\\GAMP"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
